<%= simple_form_for(@admin_user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :createdate %>
    <%= f.input :modifydate %>
  </div>

  <div class="form-actions pull-right">
    <%= f.button :submit  %>
  </div>
<% end %>
